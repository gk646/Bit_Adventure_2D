 public void checkCollisionEntity(ENTITY entity) {
        long time = System.nanoTime();
        for (Rectangle rect : objects_tutorial) {
            if (rect != null) {
                if (rect.intersects(entity.worldX + mg.entity.entityMovementSpeed, entity.worldY, 48, 48)) {
                    mg.entity.collisionRight = true;
                }
                if (rect.intersects(entity.worldX - mg.entity.entityMovementSpeed, entity.worldY, 48, 48)) {
                    mg.entity.collisionLeft = true;
                }
                if (rect.intersects(entity.worldX, entity.worldY - mg.entity.entityMovementSpeed, 48, 48)) {
                    mg.entity.collisionUp = true;
                }
                if (rect.intersects(entity.worldX, entity.worldY + mg.entity.entityMovementSpeed, 48, 48)) {
                    mg.entity.collisionDown = true;
                }
            }
        }